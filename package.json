{
  "name": "@espressotutorialsgmbh/espresso-tailwind-config",
  "version": "0.0.13",
  "author": "Espresso Tutorials",
  "keywords": [
    "tailwind",
    "tailwindcss",
    "tailwind-css",
    "preset",
    "espressotutorials"
  ],
  "description": "The espresso-tailwindcss-config library offers a basic tailwindcss preset. To show the Espresso Tutorials corporate identity across projects.",
  "main": "index.js",
  "scripts": {
    "build": "babel --presets env src -d ./",
    "dev": "babel --presets env src -d ./ --watch",
    "release": "read -p 'GITHUB_TOKEN: ' GITHUB_TOKEN && export GITHUB_TOKEN=$GITHUB_TOKEN && release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/espressotutorials/et-tailwindcss-config.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/espressotutorials/et-tailwindcss-config/issues"
  },
  "homepage": "https://github.com/espressotutorials/et-tailwindcss-config#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "dotenv-cli": "^7.1.0",
    "postcss": "^8.4.21",
    "release-it": "^15.5.0",
    "tailwindcss": "^3.3.0"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9"
  },
  "release-it": {
    "github": {
      "release": true
    }
  }
}
